{
    "Serilog": {
        "Using": [ "Serilog.Sinks.Console" ],
        "MinimumLevel": {
            "Default": "Warning"
        },
        "WriteTo": [
            {
                "Name": "Console",
                "Args": {
                    "outputTemplate": "[{Timestamp:HH:mm:ss} {Level}] {Message}{NewLine}{Exception}"
                }
            }
        ]
    },
    "ConnectionStrings": {
        "DefaultConnection": "",
        "Redis": ""
    },
    "Jwt": {
        "Key": "",
        "Issuer": "",
        "Audience": ""
    },
    "Cors": {
        "AllowedOrigins": [
            "https://my-frontend-app.com",
            "https://admin.my-frontend.com"
        ]
    },
    "AllowedHosts": "*"
}
